{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "A-Shell",
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.ashell",
					"match": "\\b(if|else|elseif|endif|xcall|call|as|XCALL|CALL|while|for|foreach|next|do|loop|repeat|return|switch|case|endswitch|exit|dimx|ordmap|defstruct|endstruct|define|DEFINE|PROGRAM)\\b"
				}
			]
		},
		"keywords_other": {
			"patterns": [
				{
					"name": "keyword.other.ashell",
					"match": "\\b(isam|using|read|function|endfunction|exitfunction|ucs|lcs|abs|pad|space|null|auto_extend|left|right|val\\()\\b"
				}
			]
		},
		"keywords_compiler": {
			"patterns": [
				{
					"name": "keyword.other.ashell",
					"match": "\\b(pragma|include|include'once|extern|auto_extern)\\b"
				}
			]
		},
		"log": {
			"patterns": [
				{
					"name": "support.class.ashell",
					"match": "\\b(fn'bug'app|FN'BUG'APPEND|FN'BUG'OPEN|fn'bug'head)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.ashell",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ashell",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"name": "constant.numeric.ashell",
			"patterns": [
				{
					"name": "constant.character.escape.ashell",
					"match": "[0-9]"
				}
			]
		},
		"comments": {
			"name": "comment.line.ashell",
			"begin": "!",
			"end": "\n",
			"patterns": [
				{
					"name": "constant.character.escape.ashell",
					"match": "\\\\."
				}
			]
		},
		"vartypes": {
			"name": "storage.type.ashell",
			"patterns": [
				{
					"name": "constant.character.escape.ashell",
					"match": "\\b(map1|map2|map3|map4|map5|map6|map7|map8|map9|MAP1|MAP2|MAP3|MAP4|MAP5|MAP6|MAP7|MAP8|MAP9)\\b"
				}
			]
		}
	},
	"scopeName": "source.ashell",
	"patterns": [
		{ "include": "#keywords" },
		{ "include": "#keywords_other" },
		{ "include": "#keywords_compiler" },
		{ "include": "#log" },
		{ "include": "#strings" },
		{ "include": "#numbers" },
		{ "include": "#comments" },
		{ "include": "#vartypes" }
	]
}